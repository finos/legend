          'insert into accountTable (id, name, "CREATE DATE", "CLOSE DATE") values (1, \'Acct 1\', \'2020-01-01\', \'2021-01-01\');\n'+
          'insert into accountTable (id, name, "CREATE DATE", "CLOSE DATE") values (2, \'Acct 2\', \'2020-01-01\', \'2025-01-01\');\n'+
